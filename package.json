{
  "name": "satusehat-sdk",
  "version": "0.0.2",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bilabror/satusehat-sdk.git"
  },
  "description": "TypeScript SDK for SATUSEHAT API",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown src/index.ts --format cjs,esm --dts --minify",
    "dev": "tsdown src/index.ts --format cjs,esm --dts --minify --watch",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist",
    "lint": "oxlint src/",
    "lint:fix": "oxlint src/ --fix",
    "format": "oxfmt src/",
    "format:check": "oxfmt --check src/",
    "prepare": "husky"
  },
  "keywords": [
    "satusehat",
    "fhir",
    "healthcare",
    "indonesia",
    "api",
    "sdk"
  ],
  "author": "Bil Abror",
  "license": "MIT",
  "dependencies": {
    "@types/fhir": "^0.0.41"
  },
  "devDependencies": {
    "@types/bun": "^1",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "oxfmt": "^0.20.0",
    "oxlint": "^1.35.0",
    "tsdown": "^0.18.3",
    "typescript": "^5.3.0"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "oxlint --fix",
      "oxfmt --write"
    ]
  }
}
